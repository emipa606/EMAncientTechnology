<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <SoundDef>
    <defName>EM_SkullTurretSound</defName>
    <label>skull turret noise</label>
    <description>noise a skull turret makes</description>
    <context>MapOnly</context>
    <eventNames />
    <subSounds>
      <li>
        <name>EM_SkullBulletSound</name>
        <onCamera>True</onCamera>
        <muteWhenPaused>True</muteWhenPaused>
        <tempoAffectedByGameSpeed>True</tempoAffectedByGameSpeed>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Guns/SkullBullet</clipPath>
          </li>
        </grains>
        <pitchRange>
          <min>1</min>
          <max>3</max>
        </pitchRange>
      </li>
    </subSounds>
  </SoundDef>
  <SoundDef>
    <defName>EM_ChargesSound</defName>
